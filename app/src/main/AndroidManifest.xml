                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
        <activity android:name=".home" android:windowSoftInputMode="adjustResize">

        </activity>
        <activity android:name=".DescCasa">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".home" />

        </activity>
        <activity
